/*
20190108_1538
Time Example calculation time
*/

#include<iostream>
#include<ctime>
using namespace std;

int main()
{
    clock_t calculation_time;

    calculation_time=clock();

    for(int i(0);i<1000;i++)
        for(int j(0);j<99999;)
    {
        j++;
    }

    calculation_time=clock()-calculation_time;
    cout<<"The Time of Calculation: "<<(float)calculation_time/CLOCKS_PER_SEC<<" second"<<endl;

    return 0;

}
